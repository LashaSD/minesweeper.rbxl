{
    "name": "Minesweeper",
    "tree": {
        "$className": "DataModel",

        "Players": {
            "$className": "Players",
            "$properties": {
                "CharacterAutoLoads": false
            }
        },
        "ServerScriptService": {
            "Server": {
                "$path": "./src/Server"
            }
        },
        "StarterPlayer": {
            "StarterPlayerScripts": {
                "Client": {
                    "$path": "./src/Client"
                }
            }
        },
        "ReplicatedStorage": {
            "Game": {
                "$className": "Folder",
                "Common": {
                    "$path": "./src/Common/",
                    "Packages": {
                        "$path": "./Packages"
                    }
                }
            }
        },
        "TextChatService": {
            "$className": "TextChatService",
            "$properties": {
                "ChatVersion": "TextChatService"
            }
        }
    }
}
